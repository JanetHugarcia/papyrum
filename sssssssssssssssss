[1mdiff --git a/packages/papyrum-ui/src/components/Sidebar/useMenu.ts b/packages/papyrum-ui/src/components/Sidebar/useMenu.ts[m
[1mnew file mode 100644[m
[1mindex 0000000..d047d84[m
[1m--- /dev/null[m
[1m+++ b/packages/papyrum-ui/src/components/Sidebar/useMenu.ts[m
[36m@@ -0,0 +1,38 @@[m
[32m+[m[32mimport { Entry } from '../Menu';[m
[32m+[m
[32m+[m[32mconst MAX_QUERY = 3;[m
[32m+[m
[32m+[m[32mexport const useMenu = ({ query, entries }) => {[m
[32m+[m[32m  if (query.length >= MAX_QUERY) {[m
[32m+[m[32m    let db = {};[m
[32m+[m[32m    for (let i = 0; i < Object.values(entries).length; i++) {[m
[32m+[m[32m      const entry = (Object.values(entries)[i] as Entry);[m
[32m+[m[32m      if (entry.name.toLocaleLowerCase().includes(query)) {[m
[32m+[m[32m        db[entry.name] = {[m
[32m+[m[32m          ...entry,[m
[32m+[m[32m          open: true[m
[32m+[m[32m        };[m
[32m+[m[32m        continue;[m
[32m+[m[32m      }[m
[32m+[m[32m      if (entry.children) {[m
[32m+[m[32m        db[entry.name] = {};[m
[32m+[m[32m        db[entry.name].children = [];[m
[32m+[m[32m        entry.children.forEach((child: Entry) => {[m
[32m+[m[32m          if (child.name.toLocaleLowerCase().includes(query)) {[m
[32m+[m[32m            db[entry.name] = {[m
[32m+[m[32m              ...db[entry.name],[m
[32m+[m[32m              name: entry.name,[m
[32m+[m[32m              open: true,[m
[32m+[m[32m            };[m
[32m+[m[32m            db[entry.name].children.push(child);[m
[32m+[m[32m          }[m
[32m+[m[32m        });[m
[32m+[m[32m        if (!db[entry.name].children.length) {[m
[32m+[m[32m          delete db[entry.name];[m
[32m+[m[32m        }[m
[32m+[m[32m      }[m
[32m+[m[32m    }[m
[32m+[m[32m    return db;[m
[32m+[m[32m  }[m
[32m+[m[32m  return entries;[m
[32m+[m[32m};[m
\ No newline at end of file[m
